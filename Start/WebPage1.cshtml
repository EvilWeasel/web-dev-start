<!DOCTYPE html>
<html>
<head>
    <title></title>
</head>
<body>
    @{
        string[] arr = { "Hans", "Otto", "Kurt" };
    }
    @for (int i = 0; i < 10; i++)
    {
        <p>@i</p>
    }
    <ul>

        @foreach (var item in arr)
        {
            <li>@item</li>
        }
    </ul>
</body>
</html>
